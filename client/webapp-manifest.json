{
  "name": "vapor",
  "description": "distributed vj app",
  "version": "0.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["app/background.js"]
    }
  },
  "permissions": [
    "identity",
    "fileSystem",
    "storage",
    "http://192.168.1.115:8080/",
    "http://192.168.1.115:8080/*"
  ],
  "icons": { "128": "favicon.png" },
  "start_url": "index.html",
  "display": "standalone",
  "orientation": "landscape"
}